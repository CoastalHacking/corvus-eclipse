
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          https://maven.apache.org/xsd/settings-1.0.0.xsd">
	<profiles>
		<profile>
			<id>tychoDebug</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation> 
			<properties>
				<debugPort>8000</debugPort>
				<profileName>tychoDebug</profileName>
				<osgi.console>1234</osgi.console>
				<!-- <tycho.testArgLine>-Dosgi.noShutdown=true</tycho.testArgLine> -->
				<!-- <tycho.testArgLine>-Dosgi.console=1234 -Dosgi.console.enable.builtin=true</tycho.testArgLine> -->
			</properties>
		</profile>
	</profiles>

	<activeProfiles>
		<activeProfile>tychoDebug</activeProfile>
	</activeProfiles>

</settings>
